  <string name="com_accountkit_phone_loading_title" description="Shown when attempting to verify number.">Verifying your number…</string>
  <string name="com_accountkit_phone_login_retry_title" description="Prompt over a box that contains a persons phone number where they should verify that it is the correct one">Confirm your mobile number</string>
  <string name="com_accountkit_phone_login_text" description="Legal text when logging in with phone.">Tap <ns1:g id="button_name">%1$s</ns1:g> to verify your account with Account Kit powered by Facebook. You don’t need a Facebook account to use Account Kit. An SMS may be sent to verify your number. Message and data rates may apply. <ns1:g id="start data info link">&lt;a href=%2$s></ns1:g>Learn how Facebook uses your info<ns1:g id="end link">&lt;/a></ns1:g>.</string>
  <string name="com_accountkit_phone_login_title" description="Prompt over an entry box where a person can enter their mobile phone number">Enter your mobile number</string>
  <string name="com_accountkit_phone_sending_code_on_fb_title" description="Shown when attempting to send code on Facebook.">Sending code on Facebook…</string>
  <string name="com_accountkit_phone_update_title" description="Prompt over an entry box where a person can enter their new mobile phone number">Enter your new mobile number</string>
