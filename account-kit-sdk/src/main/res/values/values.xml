  <string name="com_accountkit_resend_email_text" description="Title when resending an email.">Send New Email</string>
  <string name="com_accountkit_resend_switch_sms" description="Switch to SMS when user currently using WhatsApp">Get an SMS code</string>
  <string name="com_accountkit_resend_switch_sms_detail" description="description of get a new SMS code when user currently using WhatsApp">Get a text message with your code.</string>
  <string name="com_accountkit_resend_switch_whatsapp" description="Switch to WhatsApp when user currently using SMS">Get a WhatsApp code</string>
  <string name="com_accountkit_resend_switch_whatsapp_detail" description="description of get a new WhatsAPP code when the user currently using SMS">Get a message sent to WhatsApp with your code.</string>
