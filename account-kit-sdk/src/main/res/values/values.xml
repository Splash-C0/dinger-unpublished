  <string name="com_accountkit_resend_check_enter_code" description="Let the user enter verification code after they check their SMS or WhatsApp inbox ">Enter Code</string>
  <string name="com_accountkit_resend_check_sms" description="Prompt for a person to check the phone number they entered before a new SMS is sent to them">Check your SMS inbox</string>
