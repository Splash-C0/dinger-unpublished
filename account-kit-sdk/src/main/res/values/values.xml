  <string name="com_accountkit_email_login_retry_title" description="Title on retry screen of email flow">Confirm your email</string>
  <string name="com_accountkit_email_login_text" description="Legal text on email login screen">Tap <ns1:g id="button_name">%1$s</ns1:g> to get an email confirmation from Account Kit by Facebook to help you use <ns1:g id="app_name">%2$s</ns1:g>. You don\'t need a Facebook account to use it. <ns1:g id="start learn link">&lt;a href=%3$s></ns1:g>Learn how Facebook uses your info<ns1:g id="end link">&lt;/a></ns1:g>.</string>
